{
  "name": "sshmon-server",
  "version": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc --outDir build && resolve-tspaths --out build",
    "build-watch": "tsc --outDir build --watch | ruby -ne 'puts $_.gsub(/\\x1Bc/, \"\")' & chokidar build/ --slient -c 'resolve-tspaths --out build'",
    "clean": "rm -rf build",
    "start": "node build/cli.js"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "chokidar": "^3.5.3",
    "http-proxy": "^1.17.0",
    "joi": "^17.8.3",
    "js-yaml": "^4.1.0",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.21",
    "redux": "^4.2.1",
    "redux-thunk": "2.4.2",
    "restify": "^11.1.0",
    "socket.io": "^4.6.2",
    "tmp": "0.2.4",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@types/http-proxy": "^1.16.2",
    "@types/js-yaml": "^4.0.5",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/lodash": "^4.14.191",
    "@types/restify": "^8.5.6",
    "@types/tmp": "0.2.3",
    "@types/yargs": "^17.0.22",
    "chokidar-cli": "^3.0.0",
    "resolve-tspaths": "^0.8.13",
    "typescript": "^4.9.5"
  }
}
