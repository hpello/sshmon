{
  "compilerOptions": {
    "jsx": "react",
    "lib": ["es2018", "dom"],
    "target": "es5",
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "baseUrl": ".",
    "paths": {
      "@/server/*": ["server/src/*"],
      "@/gui/*": ["gui/src/*"]
    }
  }
}
